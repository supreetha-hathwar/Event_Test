<nav class="navbar navbar-default header">
 <div class="container-fluid">
   <div class="navbar-header">
     <a class="navbar-brand" href="#">
    <%= image_tag("logo2.png") %></a>
   </div>
   <ul class="nav navbar-nav navbar-right">
     <li class="active">
      
     <li><%= link_to "<button class=btn btn-success>Log in</button>".html_safe%>
     </li>    </ul>
 </div>
</nav></br>

<div class="container">
<div class="row">
<div class="col-md-3"></div>

<div class="col-md-6" style="background-color:green">
<h1>Sign Up</h1>


<%= form_for @user,:html => {:id => "foo"},url: user_create_path do |f| %>
  <% if @user.errors.any? %>
    <div class="error_messages">
      <h2>Form is invalid</h2>
      <ul>
        <% for message in @user.errors.full_messages %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
   <p>
    <%= f.label :FirstName %><br />
    <%= f.text_field :firstname %>
  </p>
   <p>
    <%= f.label :LastName %><br />
    <%= f.text_field :lastname %>
  </p>
   <p>
    <%= f.label :Username %><br />
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
   <p>
    <%= f.label :PhoneNumber %><br />
    <%= f.text_field :phonenumber %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation %>
  </p>
  <p class="button"><%= f.submit :OK %></p>
  <p class="button"><%= f.submit :Cancel %></p>
<% end %>
</div>
<div class="col-md-3">
</div>
</div>
</div>