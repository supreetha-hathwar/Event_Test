<div class="container">
<div class="row">
<div class="col-md-3">
  <p><h3>Event:Trip</h3></p>
  <p><h4>Organizer:Supreetha</h4></p>
  <p><h5>Partcipants</h5></p>
</div>

<div class="col-md-6" style="background-color:brown">
<h1>Create Event</h1>


<%= form_for @event,url: event_create_path do |f| %>
  
   <p>
    <%= f.label :Event %><br />
    <%= f.text_field :event_title %>
  </p>
   <p>
    <%= f.label :Venue %><br />
    <%= f.text_field :venue %>
  </p>
   <p>
    <%= f.label :Date%><br />
    <%= f.text_field :date %>
  </p>
  <p>
    <%= f.label :Time %><br />
    <%= f.text_field :time %>
  </p>
  <p>
    <%= f.label :Invitees %><br />
    <%= f.text_field :invitees %>
  </p>
   <p>
    <%= f.label :Description %><br />
    <%= f.text_area :description %>
  </p>
   <p>
    <%= f.label :Status %><br />
    <%= f.text_field :status %>
  </p>
  
   
  <p class="button"><%= f.submit :OK %></p>
  <p class="button"><%= f.submit :Cancel %></p>
<% end %>
</div>
<div class="col-md-3">
</div>
</div>
